  <connectionStrings>
    <add name="AoschMESEntities" connectionString="metadata=res://*/AoschMesModel.csdl|res://*/AoschMesModel.ssdl|res://*/AoschMesModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.;initial catalog=MES;user id=sa;password=wenyuan9009;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <system.web>
